fmod FOO is
  protecting BOOL .
  sorts Foo Bar .
  subsort Bar < Foo .
  op a : -> Foo .
  op b : -> Foo .
  op c : -> Foo .
  op f : Foo Foo -> Foo [assoc comm] .
endfm

xmatch f(X:Foo, Y:Foo) <=? f(a, b, c, c) .
